%%sinusoiids (lab3 not demo) 
t_sin=2:0.01:4;
 A0=3;A=3;T=(5-3.5)/6;f=1/T;
 delay=3.5;
 y_sin=A0+A*sin(2*pi*f*(t_sin-delay));
 plot(t_sin,y_sin)
